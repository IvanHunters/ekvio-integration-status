{
  "name": "ekvio-dev/ekvio-integration-status",
  "description": "Ekvio status integration api",
  "authors": [
    {
      "name": "Ivan Okhotnikov",
      "email": "ohotnikov.ivan@e-queo.com"
    }
  ],
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "commercial",
  "require": {
    "ekvio-dev/integration-app-skeleton": "^5",
    "ekvio-dev/integration-php-sdk": "^2",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "vimeo/psalm": "^4.0",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf"
  }
}
